{
  "name": "vuepress-plugin-docgen",
  "version": "0.10.0",
  "description": "Vuepress plugin for document your component",
  "scripts": {
    "docs": "vuepress dev docs --no-clear-screen",
    "docs:build": "vuepress build docs",
    "test": "mocha --recursive --exclude node_modules --exclude src",
    "eslint": "eslint",
    "eslint:fix": "yarn run eslint -- --fix ./src",
    "prettier": "prettier --write **/*.js",
    "commit": "git-cz"
  },
  "main": "src/index.js",
  "repository": {
    "url": "f3ltron/vuepress-plugin-docgen",
    "type": "git"
  },
  "keywords": [
    "vuepress",
    "vuepress-plugin-docgen",
    "vuejs",
    "documentation",
    "docgen"
  ],
  "author": "f3ltron<florent.giraud.16@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.2.0",
    "chokidar-cli": "^1.2.2",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "husky": "^1.2.0",
    "mocha": "^6.1.4",
    "prettier": "^1.15.2",
    "vue": "2.6.10",
    "vuepress": "^1.0.0-alpha.48",
    "vuepress-plugin-live": "^0.0.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run test"
    }
  },
  "dependencies": {
    "@vue/component-compiler-utils": "^2.6.0",
    "consola": "^2.5.6",
    "handlebars": "^4.1.2",
    "node-dir": "^0.1.17",
    "vue-docgen-api": "^3.13.4",
    "vue-template-compiler": "^2.6.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
