{
  "name": "vuepress-plugin-docgen",
  "version": "1.1.0",
  "description": "Vuepress plugin for document your component",
  "scripts": {
    "docs": "vuepress dev docs --no-clear-screen",
    "docs:build": "vuepress build docs",
    "test": "mocha --recursive --exclude node_modules --exclude src",
    "eslint": "eslint",
    "eslint:fix": "yarn run eslint -- --fix ./src",
    "prettier": "prettier --write **/*.js",
    "commit": "git-cz"
  },
  "bugs": {
    "url": "https://github.com/f3ltron/vuepress-plugin-docgen/issues"
  },
  "homepage": "https://f3ltron.github.io/vuepress-plugin-docgen/",
  "files": [
    "src",
    "README.md"
  ],
  "main": "src/index.js",
  "repository": {
    "url": "git+https://github.com/f3ltron/vuepress-plugin-docgen.git",
    "type": "git"
  },
  "keywords": [
    "vuepress",
    "vuepress-plugin-docgen",
    "vuejs",
    "documentation",
    "docgen"
  ],
  "author": "f3ltron<florent.giraud.16@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.2.0",
    "chokidar-cli": "^1.2.2",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "glob": "^7.1.4",
    "husky": "^1.2.0",
    "mocha": "^6.1.4",
    "node-sass": "^4.12.0",
    "prettier": "^1.15.2",
    "sass-loader": "^7.1.0",
    "vue": "2.6.10",
    "vuepress": "^1.0.0-alpha.48",
    "vuepress-plugin-live": "^1.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run test"
    }
  },
  "dependencies": {
    "@vue/component-compiler-utils": "^2.6.0",
    "consola": "^2.5.6",
    "deepmerge": "^3.2.0",
    "vue-docgen-api": "^3.13.4",
    "vue-template-compiler": "^2.6.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
